{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "2.0",
    "description": "A simple extension",
    "content_scripts" : [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "all_frames": true
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}x